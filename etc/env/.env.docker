API_SESSION_NAME=api-session
API_SESSION_KEYS=secret
COOKIE_SESSION_NAME=cookie-sesion
COOKIE_SESSION_KEYS=secret
COOKIE_SESSION_EXPIRY_MS=3600000

REDIS_URL=redis://redis:6379